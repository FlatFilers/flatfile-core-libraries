    "md5": "^2.3.0",
    "typescript": "^4.7.4"
  },
  "author": "",
  "devDependencies": {
    "@flatfile/eslint-config-platform-sdk": "*",
    "@flatfile/ts-config-platform-sdk": "*",
    "@types/jest": "^28.1.0",
    "@types/lodash": "^4.14.182",
    "@types/md5": "^2.3.2",
    "dotenv": "^8.2.0",
    "eslint": "^8.18.0",
    "jest": "^28.1.0",
    "ts-jest": "^28.0.4",
    "tsup": "^6.1.2"
  }
