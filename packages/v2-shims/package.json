{
  "name": "@flatfile/v2-shims",
  "version": "0.0.1",
  "description": "A collection of helpers for migrating from v2 to platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --dts  --watch",
    "test": "jest --passWithNoTests",
    "prepublish": "npm run build"
  },
  "files": [
    "dist/**"
  ],
  "author": "David Boskovic",
  "license": "ISC",
  "devDependencies": {
    "@flatfile/eslint-config-platform-sdk": "*",
    "@flatfile/jest-preset-platform-sdk": "*",
    "@flatfile/ts-config-platform-sdk": "*",
    "@types/async": "^3.2.21",
    "@types/jest": "^29.5.1",
    "eslint": "^8.19.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.4.1",
    "tsup": "^6.1.3",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@flatfile/blueprint": "^0.0.9",
    "@flatfile/listener": "^0.3.15",
    "@flatfile/api": "^1.5.27",
    "@flatfile/plugin-record-hook": "^1.1.4",
    "@flatfile/utils-testing": "^0.0.3"
  }
}