{
  "name": "@flatfile/react",
  "version": "7.2.21",
  "description": "Flatfile React components",
  "main": "./dist/index.mjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "default": "./dist/index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlatFilers/platform-sdk-mono.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "git+https://github.com/FlatFilers/platform-sdk-mono/issues"
  },
  "homepage": "https://github.com/FlatFilers/platform-sdk-mono#readme",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup index.ts --format esm,cjs --dts",
    "dev": "tsup index.ts --format esm,cjs --watch",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest --runInBand --forceExit --passWithNoTests"
  },
  "devDependencies": {
    "@flatfile/eslint-config-platform-sdk": "*",
    "@flatfile/ts-config-platform-sdk": "*",
    "@rollup/plugin-typescript": "^11.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^28.1.4",
    "@types/pubnub": "^7.2.1",
    "@types/react": ">=16.9.0 <19.0.0",
    "@types/react-dom": ">=16.9.0 <19.0.0",
    "@types/styled-components": "^5.1.26",
    "@types/tinycolor2": "^1.4.3",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^29.6.4",
    "jsdom": "^21.1.0",
    "react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-hooks-testing-library": "^0.6.0",
    "react-router-dom": "^6.7.0",
    "ts-jest": "^28.0.5"
  },
  "dependencies": {
    "@flatfile/api": "1.5.20",
    "@flatfile/cross-env-config": "^0.0.5",
    "@flatfile/hooks": "^1.3.2",
    "@flatfile/listener": "0.3.15",
    "@flatfile/plugin-record-hook": "^0.1.10",
    "@flatfile/embedded-utils": "^1.0.4",
    "pubnub": "^7.2.2",
    "pubnub-react": "^3.0.2",
    "react-test-renderer": "^18.2.0",
    "tinycolor2": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "styled-components": "^5.3.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "overrides": {
    "pubnub@^7.2.2": {
      "superagent-proxy@^3.0.0": {
        "proxy-agent@^5.0.0": {
          "pac-proxy-agent@^5.0.0": {
            "pac-resolver@^5.0.0": {
              "degenerator": "^5.0.0"
            }
          }
        }
      }
    }
  }
}
