{
  "name": "@flatfile/react",
  "version": "7.12.7",
  "description": "Flatfile React components",
  "type": "module",
  "exports": {
    ".": {
      "node": {
        "types": {
          "import": "./dist/index.d.ts",
          "require": "./dist/index.d.cts"
        },
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "browser": {
        "types": {
          "import": "./dist/index.d.ts",
          "require": "./dist/index.d.cts"
        },
        "import": "./dist/index.browser.js",
        "require": "./dist/index.browser.cjs"
      },
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "author": "",
  "license": "ISC",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "check-exports": "attw --pack .",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest --runInBand --forceExit --passWithNoTests"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.16.4",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@flatfile/eslint-config-platform-sdk": "*",
    "@flatfile/ts-config-platform-sdk": "*",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.2.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^28.1.4",
    "@types/react": ">=16.9.0 <19.0.0",
    "@types/react-dom": ">=16.9.0 <19.0.0",
    "@types/tinycolor2": "^1.4.3",
    "dotenv": "^16.3.2",
    "esbuild-sass-plugin": "^3.3.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jsdom": "^21.1.0",
    "ts-jest": "^28.0.5",
    "tsup": "^8.3.0"
  },
  "dependencies": {
    "@flatfile/api": "^1.9.19",
    "@flatfile/cross-env-config": "^0.0.6",
    "@flatfile/embedded-utils": "^1.3.3",
    "@flatfile/listener": "^1.0.5",
    "@flatfile/plugin-record-hook": "^1.5.2",
    "tinycolor2": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
