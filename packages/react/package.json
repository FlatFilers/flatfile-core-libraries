{
  "name": "@flatfile/react",
  "version": "7.12.3",
  "description": "Flatfile React components",
  "type": "module",
  "browser": {
    "./dist/index.cjs": "./dist/index.browser.cjs",
    "./dist/index.mjs": "./dist/index.browser.mjs"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "default": "./dist/index.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "author": "",
  "license": "ISC",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup --config",
    "dev": "rollup --config --watch",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest --runInBand --forceExit --passWithNoTests"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@flatfile/eslint-config-platform-sdk": "*",
    "@flatfile/ts-config-platform-sdk": "*",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-sucrase": "^5.0.2",
    "@rollup/plugin-url": "^8.0.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.2.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^28.1.4",
    "@types/react": ">=16.9.0 <19.0.0",
    "@types/react-dom": ">=16.9.0 <19.0.0",
    "@types/tinycolor2": "^1.4.3",
    "dotenv": "^16.3.2",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jsdom": "^21.1.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-import-css": "^3.3.5",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-terser": "^1.0.3",
    "ts-jest": "^28.0.5"
  },
  "dependencies": {
    "@flatfile/api": "^1.8.5",
    "@flatfile/cross-env-config": "^0.0.6",
    "@flatfile/embedded-utils": "^1.2.4",
    "@flatfile/listener": "^1.0.5",
    "@flatfile/plugin-record-hook": "^1.5.2",
    "tinycolor2": "^1.6.0"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
