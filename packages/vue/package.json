{
  "name": "@flatfile/vue",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist/**"
  ],
  "main": "./dist/vue.umd.js",
  "module": "./dist/vue.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue.es.js",
      "require": "./dist/vue.umd.js"
    },
    "./dist/vue.css": {
      "import": "./dist/vue.css",
      "require": "./dist/vue.css"
    }
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@flatfile/api": "^1.5.20",
    "@flatfile/embedded-utils": "^1.0.4",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3",
    "path": "^0.12.7",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0",
    "vite-plugin-libcss": "^1.1.1",
    "vue-tsc": "^1.8.5"
  }
}
