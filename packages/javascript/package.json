{
  "name": "@flatfile/javascript",
  "version": "0.1.6",
  "description": "Flatfile embedded with vanilla javascript.",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">= 12"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "license": "ISC",
  "bugs": {
    "url": "git+https://github.com/FlatFilers/platform-sdk-mono/issues"
  },
  "homepage": "https://github.com/FlatFilers/platform-sdk-mono#readme",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --format esm,cjs --watch",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest --runInBand --forceExit --passWithNoTests"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "typescript": "4.4.4",
    "@flatfile/embedded-utils": "*"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "keywords": [
    "javascript",
    "starter"
  ]
}