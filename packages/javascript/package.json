{
  "name": "@flatfile/javascript",
  "version": "1.3.6",
  "description": "Flatfile embedded with vanilla javascript.",
  "type": "module",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "default": "./dist/index.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">= 12"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "license": "ISC",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup --config",
    "dev": "rollup --config --watch",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest --runInBand --forceExit --passWithNoTests"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-sucrase": "^5.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-url": "^8.0.2",
    "dotenv": "^16.3.1",
    "jest": "^29.7.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-import-css": "^3.3.5",
    "tsup": "^7.2.0"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "keywords": [
    "javascript",
    "starter"
  ],
  "dependencies": {
    "@flatfile/embedded-utils": "^1.2.3",
    "@flatfile/listener": "^1.0.4",
    "@flatfile/plugin-record-hook": "^1.5.2"
  }
}
