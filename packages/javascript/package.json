{
  "name": "@flatfile/javascript",
  "version": "0.2.2",
  "description": "Flatfile embedded with vanilla javascript.",
  "source": "src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">= 12"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead"
  ],
  "license": "ISC",
  "bugs": {
    "url": "git+https://github.com/FlatFilers/platform-sdk-mono/issues"
  },
  "homepage": "https://github.com/FlatFilers/platform-sdk-mono#readme",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "rollup --config",
    "dev": "rollup --config --watch",
    "lint": "TIMING=1 eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "jest --runInBand --forceExit --passWithNoTests"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-url": "^8.0.2",
    "jest": "^29.7.0",
    "rollup-plugin-import-css": "^3.3.5",
    "tsup": "^7.2.0",
    "typescript": "4.4.4"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "keywords": [
    "javascript",
    "starter"
  ],
  "dependencies": {
    "@flatfile/embedded-utils": "^1.0.9",
    "@flatfile/listener": "^0.3.16",
    "@flatfile/plugin-record-hook": "^1.2.0",
    "pubnub": "^7.3.1"
  }
}